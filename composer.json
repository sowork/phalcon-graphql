  "keywords": ["phalcon", "graphql"],
  "require": {
    "php": ">= 7.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-phalcon": ">=3.3.0",
    "webonyx/graphql-php": "^0.12",
    "sowork/phalcon-eagerload": "dev-master"
  },
  "autoload": {
